      "arcade_units_adding_tickets_cost": {
        "is_enabled": false,
        "comments": "",
        "props": {
          "actionsType": "PERFORM_ONE_BY_ONE",
          "conditionsType": "ALL",
          "enableAfterComplete": true
        },
        "events": {
          "periodicEvent": {
            "time": 0.1
          }
        },
        "conditions": {
          "unitWhenCountMatches": [
            {
              "object_type": "isActive"
            },
            {
              "object_marking": 0
            },
            {
              "func": "more"
            },
            {
              "value": 0
            },
            {
              "object": "t1_quest_units_squad"
            },
            {
              "object": "t2_quest_units_squad"
            },
            {
              "var": "additional_units_quantity"
            }
          ]
        },
        "actions": [
          {
            "comment": {
              "value": "считаем количество активных юнитов в обеих командах"
            }
          },
          {
            "comment": {
              "value": "считаем только quest_units, так как они включают в себя доп сквады"
            }
          },
          {
            "comment": {
              "value": "увеличиваем переменную подсчета"
            }
          },
          {
            "varModify": {
              "operator": "add",
              "value": 1.0,
              "var": "additional_units_quantity"
            }
          }
        ],
        "else_actions": [
          {
            "comment": {
              "value": "подсчитали количество юнитов"
            }
          },
          {
            "comment": {
              "value": "поделим число на 2"
            }
          },
          {
            "varModify": {
              "operator": "divAB",
              "value": 2.0,
              "var": "additional_units_quantity"
            }
          },
          {
            "comment": {
              "value": "получим количество текущих очков команды А"
            }
          },
          {
            "varSetInt": {
              "value": 0,
              "var": "additional_units_cur_tickets",
              "source_var": "mpTi1"
            }
          },
          {
            "varSetInt": {
              "value": 0,
              "var": "additional_units_tickets_cost"
            }
          },
          {
            "varModify": {
              "operator": "add",
              "value": 0.0,
              "var": "additional_units_tickets_cost",
              "use_variable": true,
              "var_value": "additional_units_cur_tickets"
            }
          },
          {
            "comment": {
              "value": "разделим количество очков на количество юнитов"
            }
          },
          {
            "varModify": {
              "operator": "divAB",
              "value": 1.0,
              "var": "additional_units_tickets_cost",
              "use_variable": true,
              "var_value": "additional_units_quantity"
            }
          },
          {
            "comment": {
              "value": "добавим 1 чтобы гарантировать нулевое количество очков при смерти всей наземки"
            }
          },
          {
            "varModify": {
              "operator": "add",
              "value": 1.0,
              "var": "additional_units_tickets_cost"
            }
          },
          {
            "comment": {
              "value": "умножаем число на -1, чтобы тикеты отнимались"
            }
          },
          {
            "varModify": {
              "operator": "mul",
              "value": -1.0,
              "var": "additional_units_tickets_cost"
            }
          },
          {
            "comment": {
              "value": "запускаем триггер окончания подсчетов"
            }
          },
          {
            "triggerEnable": {
              "target": "arcade_units_adding_tickets_end"
            }
          }
        ]
