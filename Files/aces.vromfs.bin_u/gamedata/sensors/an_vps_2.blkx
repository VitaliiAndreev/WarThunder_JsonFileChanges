{
  "type": "radar",
  "transivers": {
    "track": {
      "power": 60.0,
      "band": 0,
      "rcs": 3.0,
      "range": 5000.0,
      "rangeMax": 7000.0,
      "backScatteringBandNum": 0,
      "antenna": {
        "angleHalfSens": 1.5,
        "sideLobesSensitivity": -35.0
      }
    }
  },
  "scanPatterns": {
    "losLock": {
      "type": "pyramide",
      "azimuthLimits": [-180.0,180.0],
      "elevationLimits": [-20.0,85.0],
      "yawStabLimit": 180.0,
      "pitchStabLimit": 10.0,
      "rollStabLimit": 10.0,
      "period": 0.25,
      "width": 2.0,
      "barHeight": 1.5,
      "barsCount": 1,
      "rowMajor": false
    },
    "track": {
      "type": "no",
      "yawStabLimit": 180.0,
      "pitchStabLimit": 10.0,
      "rollStabLimit": 10.0,
      "azimuthLimits": [-180.0,180.0],
      "elevationLimits": [-20.0,85.0]
    }
  },
  "signals": {
    "track": {
      "absRelSpeed": true,
      "distance": {
        "presents": true,
        "minValue": 100.0,
        "maxValue": 7000.0
      },
      "relSpeed": {
        "presents": true,
        "minValue": 15.0,
        "maxValue": 1000.0,
        "signalWidthMin": 3.0,
        "width": 0.0
      }
    }
  },
  "fsms": {
    "main": {
      "stateInit": "init",
      "actionsTemplates": {
        "setStandbyMode": {
          "setTransiver": [],
          "setScanPattern": {
            "scanPattern": "losLock"
          },
          "setSignal": {
            "signal": "track"
          },
          "setCenterAzimuth": {
            "source": "constant",
            "value": 0.0
          },
          "setCenterElevation": {
            "source": "constant",
            "value": 0.0
          },
          "clearTargetsOfInterest": []
        },
        "setLosLockMode": {
          "setTransiver": {
            "transiver": "track"
          },
          "setScanPattern": {
            "scanPattern": "losLock"
          },
          "resetScanPhase": [],
          "setCenterAzimuth": {
            "source": "targetDesignation"
          },
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setDistGatePos": {
            "source": "constRange",
            "pos": 3500.0,
            "width": 7000.0
          },
          "clearTargetsOfInterest": [],
          "setFsmActive": {
            "fsm": "losLock",
            "active": true
          }
        },
        "setTrackMode": {
          "setTransiver": {
            "transiver": "track"
          },
          "setScanPattern": {
            "scanPattern": "track"
          },
          "clearTargetsOfInterest": [],
          "addTargetOfInterest": [],
          "setLastTargetOfInterestActive": [],
          "updateActiveTargetOfInterest": [],
          "setCenterAzimuth": {
            "source": "activeTargetOfInterest"
          },
          "setCenterElevation": {
            "source": "activeTargetOfInterest"
          },
          "setDistGatePos": {
            "source": "activeTargetOfInterest",
            "width": 0.0
          },
          "setFsmActive": {
            "fsm": "track",
            "active": true
          }
        }
      },
      "transitions": {
        "switchOn": {
          "stateFrom": "init",
          "event": "init",
          "stateTo": "standby",
          "actions": {
            "setStandbyMode": []
          }
        },
        "checkLockType": {
          "stateFrom": "standby",
          "command": "designateTarget",
          "stateTo": "checkLockType",
          "actions": {
            "checkDesignationTypeEquals": {
              "value": 1
            }
          }
        },
        "losLock": {
          "stateFrom": "checkLockType",
          "event": "designationTypeEquals",
          "stateTo": "losLock",
          "actions": {
            "setLosLockMode": []
          }
        },
        "finishLockTypeCheck": {
          "stateFrom": "checkLockType",
          "event": "designationTypeNotEquals",
          "stateTo": "standby"
        },
        "track": {
          "stateFrom": "losLock",
          "event": "targetDetected",
          "stateTo": "track",
          "actions": {
            "setFsmActive": {
              "fsm": "losLock",
              "active": false
            },
            "setTrackMode": []
          }
        },
        "failedLock": {
          "stateFrom": "losLock",
          "event": "scanFinished",
          "stateTo": "standby",
          "actions": {
            "setFsmActive": {
              "fsm": "losLock",
              "active": false
            },
            "setStandbyMode": []
          }
        },
        "finishedTrack": [
          {
            "stateFrom": "track"
          },
          {
            "event": "targetNotDetected"
          },
          {
            "command": "selectTarget"
          },
          {
            "command": "designateTarget"
          },
          {
            "stateTo": "standby"
          },
          {
            "actions": {
              "setFsmActive": {
                "fsm": "track",
                "active": false
              },
              "setStandbyMode": []
            }
          }
        ]
      }
    },
    "losLock": {
      "stateInit": "losLock",
      "transitions": {
        "scan": {
          "event": "update",
          "actions": {
            "scan": []
          }
        },
        "detect": {
          "event": "targetInSight",
          "actions": {
            "detectTarget": {
              "groundClutter": false,
              "groundAsTarget": false
            }
          }
        }
      }
    },
    "track": {
      "stateInit": "track",
      "transitions": {
        "detect": {
          "event": "update",
          "actions": {
            "detectTarget": {
              "groundClutter": false,
              "groundAsTarget": false
            }
          }
        },
        "track": {
          "event": "targetDetected",
          "actions": {
            "updateActiveTargetOfInterest": [],
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          }
        }
      }
    }
  }
}