{
  "root": {
    "background-color": "@shadeBackgroundColor",
    "type": "big",
    "frame": {
      "size": "0.8*@scrn_tgt, 0.5@scrn_tgt",
      "pos": "0.5pw-0.5w, 1@titleLogoPlateHeight + 0.3*(sh - 1@titleLogoPlateHeight - h)",
      "position": "absolute",
      "flow": "vertical",
      "class": "wndNav",
      "frame_header": {
        "activeText": {
          "id": "clan_title_table",
          "caption": "yes",
          "text": "#clan/candidatesList"
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "table": {
        "id": "candidatesList",
        "width": "pw",
        "height": "fh",
        "class": "lbTable",
        "behavior": "TableNavigator",
        "text-valign": "center",
        "text-halign": "center",
        "navigatorShortcuts": "yes",
        "focus": "yes",
        "on_click": "onSelect",
        "_on_r_click": "onUserRClick",
        "on_dbl_click": "onUserCard",
        "cur_row": "1",
        "cur_col": "0",
        "num_rows": "-1",
        "on_wrap_up": "onWrapUp",
        "on_wrap_down": "onWrapDown",
        "padding-bottom": "0.02*@scrn_tgt"
      },
      "navBar": {
        "navLeft": [
          {
            "Button_text": {
              "id": "btn_reject",
              "text": "#clan/requestReject",
              "display": "hide",
              "_on_click": "onRequestReject",
              "ButtonImg": []
            }
          },
          {
            "Button_text": {
              "id": "btn_user_options",
              "text": "#mainmenu/btnUserAction",
              "btnName": "X",
              "display": "hide",
              "_on_click": "onUserAction",
              "ButtonImg": []
            }
          }
        ],
        "navMiddle": {
          "id": "paginator_place"
        },
        "navRight": [
          {
            "Button_text": {
              "id": "btn_removeBlacklist",
              "text": "#msgbox/btn_delete",
              "_on_click": "onDeleteFromBlacklist",
              "display": "hide",
              "btnName": "A",
              "ButtonImg": []
            }
          },
          {
            "Button_text": {
              "id": "btn_approve",
              "visualStyle": "secondary",
              "text": "#clan/requestApprove",
              "display": "hide",
              "_on_click": "onRequestApprove",
              "ButtonImg": [],
              "buttonWink": []
            }
          }
        ]
      }
    }
  }
}