[
  {
    "padding": "1@tooltipPadding"
  },
  {
    "div": {
      "id": "award_image_sizer",
      "size": "@profileUnlockIconSize, @profileUnlockIconSize",
      "layeredIconContainer": {
        "id": "award_image",
        "size": "ph, ph",
        "pos": "50%pw-50%w, 50%ph-50%h",
        "position": "absolute"
      },
      "img": {
        "id": "award_multiplier",
        "background-image": "#ui/gameuiskin#window_body_shadow",
        "pos": "ph-w, ph-h + 0.05ph",
        "position": "absolute",
        "display": "hide",
        "textarea": {
          "id": "amount_text",
          "caption": "yes",
          "overlayTextColor": "userlog",
          "text": "",
          "removeParagraphIndent": "yes",
          "padding": "0.05ph"
        }
      }
    }
  },
  {
    "div": [
      {
        "pos": "1@tooltipPadding, 0"
      },
      {
        "position": "relative"
      },
      {
        "flow": "vertical"
      },
      {
        "width": "pw - 1@profileUnlockIconSize -1@tooltipPadding"
      },
      {
        "textareaNoTab": {
          "id": "award_title_text",
          "width": "pw"
        }
      },
      {
        "textareaNoTab": {
          "id": "unlock_name",
          "width": "pw",
          "pare-text": "yes",
          "overlayTextColor": "unlockHeader",
          "padding-top": "6@sf/@pf"
        }
      },
      {
        "textareaNoTab": {
          "id": "desc_text",
          "smallFont": "yes",
          "width": "pw",
          "padding-top": "3@sf/@pf"
        }
      },
      {
        "challengeDescriptionProgress": {
          "id": "progress",
          "max-width": "pw",
          "value": "",
          "display": "hide",
          "margin-left": "1@dp"
        }
      },
      {
        "textareaNoTab": {
          "id": "mult_awards_text",
          "smallFont": "yes",
          "width": "pw",
          "padding-top": "1@tooltipPadding"
        }
      },
      {
        "textareaNoTab": {
          "id": "award_text",
          "smallFont": "yes",
          "width": "pw"
        }
      },
      {
        "div": {
          "id": "state",
          "flow": "horizontal",
          "width": "pw",
          "padding-top": "6@sf/@pf",
          "display": "hide",
          "cardImg": {
            "id": "state_icon",
            "pos": "-20%w, 0",
            "size": "@unlockIconSize, @unlockIconSize",
            "re-type": "9rect",
            "background-repeat": "expand",
            "background-image": "#ui/gameuiskin#favorite",
            "margin": "0, 0, 8@sf/@pf, 0"
          },
          "tdiv": {
            "width": "fw",
            "pos": "0, ph/2-h/2",
            "position": "relative",
            "flow": "h-flow",
            "textareaNoTab": {
              "id": "state_text",
              "width": "pw",
              "smallFont": "yes",
              "hideEmptyText": "yes"
            }
          }
        }
      }
    ]
  }
]