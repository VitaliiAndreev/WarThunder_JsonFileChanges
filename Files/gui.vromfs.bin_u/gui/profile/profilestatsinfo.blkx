{
  "flow": "vertical",
  "HorizontalListBox": {
    "id": "modes_list",
    "height": "4*@scrn_tgt/100.0",
    "pos": "0, 0",
    "position": "relative",
    "activeAccesskeys": "LeftRight",
    "navigatorShortcuts": "yes",
    "on_wrap_up": "onWrapUp",
    "on_wrap_down": "onWrapDown",
    "on_select": "onStatsModeChange"
  },
  "tdiv": [
    {
      "width": "pw"
    },
    {
      "tdiv": {
        "id": "countries_boxes",
        "margin": "1@blockInterval, 1@blockInterval",
        "behaviour": "wrapNavigator",
        "navigatorShortcuts": "yes",
        "childsActivate": "yes",
        "on_wrap_up": "onWrapUp",
        "on_wrap_down": "onWrapDown",
        "on_wrap_right": "onWrapDown"
      }
    },
    {
      "tdiv": {
        "id": "units_boxes",
        "pos": "pw-w, 0",
        "position": "absolute",
        "margin": "1@blockInterval, 1@blockInterval",
        "behaviour": "wrapNavigator",
        "navigatorShortcuts": "yes",
        "childsActivate": "yes",
        "on_wrap_up": "onWrapUp",
        "on_wrap_down": "onWrapDown",
        "on_wrap_left": "onWrapUp"
      }
    }
  ],
  "frameBlock": [
    {
      "size": "pw, fh"
    },
    {
      "position": "relative"
    },
    {
      "pos": "50%pw-50%w, 0"
    },
    {
      "position": "relative"
    },
    {
      "padding-top": "1@blockInterval"
    },
    {
      "table": {
        "id": "airs_stats_table",
        "size": "pw, ph",
        "pos": "50%pw-50%w, 0",
        "position": "relative",
        "class": "lbTable",
        "higlightRowOnHover": "yes",
        "text-valign": "center",
        "text-halign": "center"
      }
    }
  ],
  "animated_wait_icon": {
    "id": "stats_loading",
    "pos": "50%pw-50%w, 50%ph-50%h",
    "position": "absolute",
    "background-rotation": "0"
  }
}