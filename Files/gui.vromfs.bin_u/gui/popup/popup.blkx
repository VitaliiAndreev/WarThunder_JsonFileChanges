{
  "width": "pw",
  "overflow": "hidden",
  "_size-timer": "0",
  "on_click": "onClickPopup",
  "on_r_click": "onRClickPopup",
  "timer_handler_func": "onTimerUpdate",
  "timer_interval_msec": "10000",
  "popupWrap": {
    "id": "popup_wrap",
    "padding": "1@framePadding",
    "class": "smallFont",
    "background-color": "@frameBackgroundColor",
    "border": "yes",
    "border-color": "@frameBorderColor",
    "border-offset": "@dp",
    "position": "absolute",
    "size": "p.p.w, p.p.h",
    "bottom": "0",
    "left": "0",
    "flow": "vertical",
    "input-transparent": "yes",
    "Button_close": {
      "id": "close_button",
      "smallIcon": "yes",
      "on_click": "onClosePopup"
    },
    "activeText": {
      "id": "title",
      "smallFont": "yes",
      "position": "relative",
      "max-width": "pw - 1@smallButtonCloseHeight",
      "pare-text": "yes",
      "text": "",
      "input-transparent": "yes"
    },
    "textAreaNoScroll": {
      "id": "msg",
      "text": "",
      "input-transparent": "yes",
      "pare-text": "yes"
    },
    "tdiv": {
      "id": "popup_buttons_place",
      "max-width": "pw",
      "pos": "pw-w, 3@dp",
      "padding": "-1@buttonMargin",
      "position": "relative"
    }
  }
}