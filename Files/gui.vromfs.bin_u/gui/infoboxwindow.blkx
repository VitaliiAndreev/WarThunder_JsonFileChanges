{
  "root": {
    "background-color": "@shadeBackgroundColor2",
    "frame": {
      "width": "1.1@sf",
      "position": "root",
      "pos": "50%pw-50%w, 50%ph-50%h",
      "flow": "vertical",
      "class": "wndNav",
      "frame_header": {
        "activeText": {
          "id": "message_box_header",
          "text": "",
          "caption": "yes"
        },
        "Button_close": []
      },
      "textarea": {
        "id": "message_box_body",
        "width": "pw",
        "max-height": "85%sh",
        "padding": "0, 0.01@sf",
        "position": "relative",
        "pos": "50%pw-50%w, 0",
