{
  "root": {
    "background-color": "@shadeBackgroundColor2",
    "frame": {
      "width": "50*@scrn_tgt/100.0",
      "pos": "50%pw-50%w, 40%ph-40%h",
      "position": "absolute",
      "flow": "vertical",
      "frame_header": {
        "activeText": {
          "id": "edit_box_window_header",
          "caption": "yes",
          "text": ""
        },
        "Button_close": {
          "id": "btn_back"
        }
      },
      "textAreaCentered": {
        "id": "editbox_header",
        "width": "pw",
        "text": "",
        "position": "relative",
        "pos": "50%pw-50%w, 1@frameHeaderHeight",
        "padding": "0.025@scrn_tgt, 0"
      },
      "tdiv": [
        {
          "width": "45*@scrn_tgt/100.0"
        },
        {
          "pos": "50%pw-50%w, 3*@scrn_tgt/100.0"
        },
        {
          "position": "relative"
        },
        {
          "EditBox": {
            "id": "edit_box_window_text",
            "width": "pw",
            "mouse-focusable": "yes",
            "display": "hide",
            "enable": "no",
            "on_activate": "onOk",
            "_on_cancel_edit": "goBack",
            "on_change_value": "onChangeValue"
          }
        },
        {
          "EditBox": {
            "id": "edit_box_window_text_multiline",
            "size": "pw, 4@buttonHeight",
            "multiline": "yes",
            "mouse-focusable": "yes",
            "display": "hide",
            "enable": "no",
            "_on_cancel_edit": "goBack",
            "on_change_value": "onChangeValue"
          }
        }
      ],
      "div": {
        "pos": "50%pw-50%w, 0.03@scrn_tgt",
        "position": "relative",
        "Button_text": {
          "id": "btn_ok",
          "text": "#msgbox/btn_ok",
          "on_click": "onOk",
          "btnName": "A",
          "ButtonImg": []
        }
      }
    }
  }
}