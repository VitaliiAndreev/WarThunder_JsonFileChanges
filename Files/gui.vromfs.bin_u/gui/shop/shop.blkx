{
  "root": {
    "background-color": "@shadeBackgroundColor2",
    "type": "shop",
    "titletext": {
      "id": "shop-title",
      "text": ""
    },
    "frame": [
      {
        "size": "@rw, sh-30*@scrn_tgt/100.0"
      },
      {
        "pos": "50%pw-50%w, sh-13*@scrn_tgt/100.0-h"
      },
      {
        "position": "absolute"
      },
      {
        "class": "wndNav"
      },
      {
        "frame_header": {
          "HorizontalListBox": {
            "id": "shop_pages_list",
            "height": "ph",
            "class": "header",
            "withImages": "yes",
            "activeAccesskeys": "RS",
            "on_select": "onAircraftsPage"
          },
          "textareaNoTab": {
            "id": "shop_header",
            "caption": "yes",
            "text": ""
          },
          "top_right_holder": {
            "hasRightIndent": "yes",
            "tdiv": {
              "id": "shop_search_box",
              "width": "0.33@scrn_tgt",
              "pos": "0, ph/2-h/2",
              "position": "relative",
              "flow": "horizontal"
            }
          },
          "Button_close": {
            "id": "btn_back",
            "style": "on_click:onCloseShop"
          }
        }
      },
      {
        "tdiv": {
          "id": "tree_header_div",
          "size": "pw, 1@buttonHeight"
        }
      },
      {
        "tdiv": {
          "size": "pw, 2@dp",
          "background-color": "@frameBackgroundColor"
        }
      },
      {
        "tdiv": [
          {
            "size": "pw, fh"
          },
          {
            "id": "shop_items_visible_div"
          },
          {
            "overflow-y": "auto"
          },
          {
            "scrollbarShortcuts": "yes"
          },
          {
            "tdiv": [
              {
                "id": "shop_items_scroll_div"
              },
              {
                "width": "fw"
              },
              {
                "tdiv": {
                  "id": "shop_useful_width",
                  "pos": "0, 0",
                  "position": "absolute",
                  "size": "pw-@scrollBarSize, 0"
                }
              },
              {
                "tdiv": {
                  "id": "shopTable_air_plates",
                  "pos": "0, 0",
                  "position": "absolute",
                  "height": "ph",
                  "flow": "vertical"
                }
              },
              {
                "tdiv": {
                  "id": "shop_items_pos_div",
                  "left": "(pw-w)/2",
                  "padding": "1@modBlockTierNumHeight, 0, 1@scrollBarSize, 0",
                  "top": "1@shop_h_extra_first",
                  "position": "relative",
                  "tdiv": {
                    "id": "shopTable_air_rows",
                    "pos": "1@modBlockTierNumHeight, 0",
                    "position": "absolute",
                    "height": "ph",
                    "flow": "vertical"
                  },
                  "table": {
                    "id": "shop_items_list",
                    "class": "shopTable",
                    "focus": "yes",
                    "behavior": "columnNavigator",
                    "cur_col": "0",
                    "cur_row": "0",
                    "fixed_row": "0",
                    "navigatorShortcuts": "yes",
                    "alwaysShowBorder": "yes",
                    "on_click": "onAircraftClick",
                    "_on_dbl_click": "onDoubleClick",
                    "on_select": "onUnitSelect",
                    "on_wrap_up": "onWrapUp",
                    "on_wrap_down": "onWrapDown"
                  }
                }
              },
              {
                "tdiv": {
                  "id": "shop_dark_screen",
                  "pos": "0, 0",
                  "position": "absolute",
                  "size": "pw, ph",
                  "total-input-transparent": "yes",
                  "display": "hide"
                }
              }
            ]
          },
          {
            "tdiv": {
              "id": "tier_arrows_div",
              "pos": "0, 1@shop_h_extra_first",
              "position": "absolute",
              "width": "1@modBlockTierNumHeight",
              "height": "ph"
            }
          }
        ]
      },
      {
        "navBar": []
      },
      {
        "dummy": {
          "id": "shop_timer",
          "behavior": "Timer",
          "timer_handler_func": "onUpdate"
        }
      }
    ]
  }
}