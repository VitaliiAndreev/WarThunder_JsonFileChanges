{
  "tdiv": {
    "id": "bg_picture_container",
    "size": "sw, sh",
    "position": "root",
    "background-color": "@black",
    "tdiv": [
      {
        "pos": "50%sw-50%w,50%sh-50%h"
      },
      {
        "position": "absolute"
      },
      {
        "size": "2.333sh, sh"
      },
      {
        "overflow": "hidden"
      },
      {
        "tdiv": {
          "id": "animated_bg_picture",
          "pos": "pw/2-w/2, ph/2-h/2",
          "position": "absolute",
          "size": "2.5sh, sh",
          "behaviour": "multiLayerImage",
          "blend-time": "500"
        }
      },
      {
        "tdiv": {
          "size": "0.333sh, sh",
          "pos": "-w/2, 0",
          "position": "absolute",
          "background-image": "!ui/gameuiskin#debriefing_bg_grad@@ss",
          "background-color": "@black"
        }
      },
      {
        "tdiv": {
          "size": "0.333sh, sh",
          "pos": "pw -w/2, 0",
          "position": "absolute",
          "background-image": "!ui/gameuiskin#debriefing_bg_grad@@ss",
          "background-color": "@black"
        }
      }
    ]
  },
  "div": [
    {
      "width": "pw"
    },
    {
      "position": "absolute"
    },
    {
      "pos": "0, 50%ph-50%h"
    },
    {
      "position": "relative"
    },
    {
      "re-type": "9rect"
    },
    {
      "background-color": "@white"
    },
    {
      "background-position": "1,5"
    },
    {
      "background-repeat": "expand"
    },
    {
      "background-image": "#ui/gameuiskin#optionlist_background_middle"
    },
    {
      "tdiv": [
        {
          "flow": "vertical"
        },
        {
          "width": "pw"
        },
        {
          "height": "ph"
        },
        {
          "position": "relative"
        },
        {
          "padding": "15"
        },
        {
          "textAreaCentered": {
            "id": "msgText",
            "width": "1.1@sf",
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "mediumFont": "yes",
            "margin": "1.5*@scrn_tgt/100.0"
          }
        },
        {
          "everyDayRewardProgress": {
            "id": "loading_progress_box",
            "position": "relative",
            "pos": "50%pw-50%w",
            "width": "0.5@sf",
            "height": "1@loopProgressHeight",
            "foreground-color": "@red",
            "pattern": {
              "type": "dark_diag_lines"
            }
          }
        },
        {
          "textAreaCentered": {
            "id": "msgTextSub",
            "width": "1.1@sf",
            "pos": "50%pw-50%w, 0",
            "position": "relative",
            "smallFont": "yes",
            "margin": "0.5*@scrn_tgt/100.0"
          }
        },
        {
          "div": [
            {
              "id": "buttons_holder"
            },
            {
              "pos": "50%pw-50%w, 0"
            },
            {
              "position": "relative"
            },
            {
              "margin": "0.01@scrn_tgt"
            },
            {
              "Button_text": {
                "id": "btn_ok",
                "text": "#msgbox/btn_ok",
                "on_click": "goBack",
                "btnName": "A",
                "ButtonImg": []
              }
            },
            {
              "Button_text": {
                "id": "btn_cancel",
                "text": "#msgbox/btn_cancel",
                "on_click": "onCancel",
                "btnName": "B",
                "ButtonImg": []
              }
            }
          ]
        }
      ]
    }
  ],
  "BottomMenuPanel": {
    "shadowSafeArea": [],
    "line": [],
    "Red_shadow_light": {
      "pos": "50%pw-50%w, 0",
      "position": "absolute",
      "size": "25%pw, ph"
    },
    "GameLogoPlate": {
      "pos": "50%pw - 50%w, 50%ph-50%h",
      "position": "absolute",
      "Shadow": {
        "redPlate": {
          "pattern": {
            "type": "dark_diag_lines",
            "tdiv": {
              "height": "1@titleLogoPlateHeight + 0.5@bh",
              "min-width": "1@titleLogoPlateWidth",
              "tdiv": {
                "pos": "50%pw-50%w, 50%ph-50%h",
                "position": "relative",
                "padding": "0.01@scrn_tgt, 0",
                "animated_wait_icon": {
                  "id": "rot",
                  "pos": "0, 50%ph-50%h",
                  "position": "relative",
                  "background-rotation": "0"
                },
                "textarea": {
                  "id": "loading",
                  "pos": "0, 50%ph-50%h",
                  "position": "relative",
                  "text": "#loading",
                  "mediumFont": "yes",
                  "overlayTextColor": "active",
                  "textShade": "yes"
                }
              }
            }
          }
        }
      }
    },
    "copyright": {
      "pos": "pw-w-1@bw, 50%ph - 50%h",
      "position": "absolute"
    }
  },
  "dummy": {
    "id": "updater_timer",
    "behavior": "Timer",
    "timer_handler_func": "onUpdate"
  }
}