{
  "tdiv": {
    "id": "bg_picture_container",
    "size": "sw, sh",
    "position": "root",
    "background-color": "@black",
    "tdiv": [
      {
        "pos": "50%sw-50%w,50%sh-50%h"
      },
      {
        "position": "absolute"
      },
      {
        "size": "2.333sh, sh"
      },
      {
        "overflow": "hidden"
      },
      {
        "tdiv": {
          "id": "animated_bg_picture",
          "pos": "pw/2-w/2, ph/2-h/2",
          "position": "absolute",
          "size": "2.5sh, sh",
          "behaviour": "multiLayerImage",
          "blend-time": "500"
        }
      },
      {
        "tdiv": {
          "size": "0.333sh, sh",
          "pos": "-w/2, 0",
          "position": "absolute",
          "background-image": "!ui/gameuiskin#debriefing_bg_grad@@ss",
          "background-color": "@black"
        }
      },
      {
        "tdiv": {
          "size": "0.333sh, sh",
          "pos": "pw -w/2, 0",
          "position": "absolute",
          "background-image": "!ui/gameuiskin#debriefing_bg_grad@@ss",
          "background-color": "@black"
        }
      }
    ]
  },
  "systemMsg": {
    "pos": "1@systemMsgIndent, 0",
    "position": "root",
    "flow": "vertical",
    "dlDataStat": {
      "behavior": "bhvUpdater",
      "pos": "50%pw-50%w,0",
      "position": "relative",
      "value": "{\"viewId\": \"DL_DATA_STATE_TEXT\"}",
      "css-hier-invalidate": "yes",
      "animText": {
        "text": "",
        "font": "@fontTiny",
        "color-factor": "0",
        "fade": "out"
      }
    }
  },
  "root": [
    {
      "tdiv": {
        "pos": "50%sw-50%w,50%sh-50%h",
        "position": "root",
        "flow": "vertical",
        "textAreaCentered": {
          "id": "msgText",
          "width": "1.1@sf",
          "mediumFont": "yes",
          "textShade": "yes",
          "overlayTextColor": "active"
        },
        "animated_wait_icon": {
          "pos": "50%pw-50%w,3*@scrn_tgt/100.0",
          "position": "relative",
          "background-rotation": "0"
        }
      }
    },
    {
      "tdiv": {
        "pos": "50%pw-50%w, sh - 1@bh - 0.07@sf - h",
        "position": "absolute",
        "flow": "vertical",
        "background-color": "@hudTextBgShade",
        "background-repeat": "expand",
        "padding": "0.1@sf, 0.02@sf",
        "textAreaCentered": {
          "class": "active",
          "max-width": "1@sf",
          "behaviour": "bhvLoadingTip"
        }
      }
    }
  ]
}